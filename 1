class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        n = len(nums)
        res = [1] * n      
        l, r = 1, 1
        for i in range(n):
            res[i] *= l
            l *= nums[i]
        for i in range(n-1, 0, -1):
            r *= nums[i]
            res[i-1] *= r
        return res
